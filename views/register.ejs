  <script>
    $('#register').click(function(){
      var v = {
      id: $("#user_id").val(),
      pw: $("#pw").val(),
      name: $("#name").val(),
      age: $("#age").val(),
      phone: $("#phone").val(),
      experience: $("#experience").val(),
      java: $("#java").val(),
      python: $("#python").val(),
      cpp: $("#cpp").val()
    }
    $.ajax({
      type: 'post',
      url: '/users/insert',
      data: v,
      success: function(data) {

        window.location.assign('/login');
    },
        error: function(err) {
          console.log(err);
        }
      });
    });
  </script>
